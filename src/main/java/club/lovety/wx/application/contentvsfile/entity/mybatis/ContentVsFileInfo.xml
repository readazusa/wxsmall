<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ContentVsFileInfo">


    <insert id="save" parameterType="ContentVsFileInfo">

      insert into t_wxsmall_content_vs_file(contentId,fileId,createTime,updateTime,syscreatetime)
      values(#{contentId},#{fileId},#{createTime},#{updateTime},now())
    </insert>


</mapper>
