<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="lazyLoadingEnabled" value="true"/>
        <setting name="aggressiveLazyLoading" value="false"/>
    </settings>

    <typeAliases>
        <typeAlias type="club.lovety.wx.application.tucao.entity.TuCaoInfo" alias="TuCaoInfo"/>
        <typeAlias type="club.lovety.wx.base.entity.BaseSearchInfo" alias="BaseSearchInfo"/>
        <typeAlias type="club.lovety.wx.base.entity.BasePageInfo" alias="BasePageInfo" />
        <typeAlias type="club.lovety.wx.application.file.entity.FileInfo" alias="FileInfo" />
        <typeAlias type="club.lovety.wx.application.contentvsfile.entity.ContentVsFileInfo" alias="ContentVsFileInfo"/>
        <typeAlias type="club.lovety.wx.application.comment.entity.CommentInfo" alias="CommentInfo"/>
        <typeAlias type="club.lovety.wx.application.comment.entity.CommentVsUser" alias="CommentVsUser"/>


    </typeAliases>

    <mappers>
        <mapper resource="club/lovety/wx/application/tucao/entity/mybatis/TuCaoInfo.xml"></mapper>
        <mapper resource="club/lovety/wx/application/file/entity/mybatis/FileInfo.xml"></mapper>
        <mapper resource="club/lovety/wx/application/contentvsfile/entity/mybatis/ContentVsFileInfo.xml"/>
        <mapper resource="club/lovety/wx/application/comment/entity/mybatis/CommentInfo.xml"/>
    </mappers>

</configuration>
 
